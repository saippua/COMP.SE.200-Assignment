{
  "name": "software-testing-assignment",
  "version": "1.0.0",
  "description": "",
  "bin": {
    "coveralls": "./bin/coveralls.js"
  },
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "mocha",
    "mochawesome": "mocha --reporter mochawesome",
    "coverage": "cross-env 'NODE_OPTIONS=--experimental-loader @istanbuljs/esm-loader-hook' nyc --all mocha",
    "test-cov": "c8 --all --reporter=lcovonly --reporter=text --include src/**/*.js npm test",
    "test-coveralls": "npm run test-cov && shx cat ./coverage/lcov.info | node ./node_modules/coveralls/bin/coveralls.js --verbose"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "c8": "^7.12.0",
    "chai": "^4.3.6",
    "coveralls": "^3.1.1",
    "mocha": "^10.1.0",
    "mochawesome": "^7.1.3",
    "shx": "^0.3.4"
  }
}
